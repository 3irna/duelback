sudo -u postgres psql


CREATE DATABASE duelbackdb

CREATE USER duelbackdbuser WITH PASSWORD 'Arya_13811218@db'
ALTER ROLE duelbackdbuser SET client-encoding TO 'utf8'
ALTER ROLE duelbackdbuser SET default_transaction_isolation TO 'read arya commit'
ALTER ROLE duelbackdbuser SET timezone TO 'UTC'
GRANT ALL PRIVILEGES ON DATABASE duelbackdb TO duelbackdbuser



